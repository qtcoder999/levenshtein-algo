{
  "ignorePatterns": [
    "dist",
    "node_modules"
  ],
  "rules": {},
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "config/webpack.common.js"
      }
    }
  }
}